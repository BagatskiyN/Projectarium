@model List<Projectarium.Domain.Entities.Request>
@{
    ViewData["Title"] = "Список заявок";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
    <link rel="stylesheet" href="~/css/ProjectManager.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
}



@if (Model != null)
{
    if (Model.Count != 0)
    {<p class="Title">Мои проекты</p>
        <div class="ButtonContainer">
            <button class="CreateButtonSmall" id="CreateButton">
                <a class="CreateButtonText">Создать проект</a>
            </button>
        </div>
        <table class="table">
            <thead>
                <tr>
                    <th>
                      Вакансия
                    </th>
                  

                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Vacancy.Title)
                        </td>
                  

                        <td>
                          
                            <a asp-action="Details" asp-route-id="@item.Id">Просмотр</a> |
                        
                            <a asp-action="Delete" asp-route-id="@item.Id">Удалить заявку</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {

        <p class="NoElementsText ">Заявок нет</p>

    }
}
else
{

    <p class="NoElementsText ">Заявок нет</p>

}  